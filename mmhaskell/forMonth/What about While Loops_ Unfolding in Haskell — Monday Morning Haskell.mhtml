.omp .omp-ad-icon { align-items: center; background-size: contain; font-size: 10px; justify-content: center; left: 30px; overflow: hidden; text-align: center; top: 30px; z-index: 3; }

.omp .omp-vast-icons { cursor: pointer; height: auto; position: absolute; z-index: 3; max-height: 100% !important; max-width: 100% !important; }

.omp .omp-thumbnail-wrapper { height: 100%; position: absolute; top: 0px; width: 100%; z-index: 10; }

.omp .omp-thumbnail-wrapper .omp-thumbnail-image { height: 100%; width: 100%; }

.omp .omp-thumbnail-wrapper .omp-thumbnail-large-play-icon { background-image: url("https://widgets.outbrain.com/images/widgetIcons/play_100x100.png"); background-position: 50% center; background-repeat: no-repeat; background-size: 12%; cursor: pointer; display: block; height: 100%; left: 0px; opacity: 0.7; position: absolute; top: 0px; width: 100%; }

.omp-native-button-wrapper { align-items: center; display: flex; max-width: 30%; }

.omp-native-button-wrapper .omp-native-button { align-items: center; border: 0.5px solid rgb(0, 0, 0); border-radius: 4px; color: rgb(0, 0, 0); cursor: pointer; display: block; font-size: 12px; font-style: normal; font-weight: 500; height: 29px; justify-content: center; line-height: 29px; overflow: hidden; padding: 0px 10px; text-decoration: none; text-overflow: ellipsis; white-space: nowrap; }

.omp-native-texts-wrapper { display: flex; flex-direction: column; gap: 6px; padding-top: 8px; }

.omp-native-texts-wrapper .omp-native-description, .omp-native-texts-wrapper .omp-native-title, .omp-native-texts-wrapper .omp-sponsor-brand { -webkit-line-clamp: 2; -webkit-box-orient: vertical; display: -webkit-box; overflow: hidden; }

.omp-native-texts-wrapper .omp-sponsor-brand { -webkit-line-clamp: 1; }

.omp-native-texts-wrapper .omp-native-title { color: rgb(0, 0, 0); font-size: 18px; font-weight: 700; }

.omp-native-texts-wrapper .omp-native-description { color: rgb(74, 73, 80); font-size: 14px; font-weight: 400; }

.omp-sponsor-wrapper { color: rgb(164, 163, 168); font-size: 12px; }

.omp-native-bottom-wrapper { gap: 6%; justify-content: space-between; padding: 8px 6px; }

.omp-native-bottom-wrapper, .omp-native-logo-wrapper { background-color: rgb(255, 255, 255); display: flex; flex-direction: row; }

.omp-native-logo-wrapper { align-items: center; gap: 6px; }

.omp-native-logo-wrapper > .omp-logo { height: 30px; width: 30px; }

.omp-native-logo-wrapper > .omp-logo > img { height: 100%; width: 100%; }

.omp-native-logo-wrapper > .omp-sponsor-brand { color: rgb(100, 100, 100); font-size: 12px; font-weight: 400; }

